#div, img, iframe
	width 300px
	display block
	border solid 16px #ddd
	margin 6px

#div
	border 0
	padding 14px
	background-color #bbb

	em
		border solid 1px red

#div, iframe
	height 100px

.e-resizer-handle-horizontal
.e-resizer-handle-vertical
	position absolute
	background-color red
	border 0
	opacity .5

.e-resizer-handle-horizontal
	height 8px
	margin-top -8px
	cursor n-resize

.e-resizer-handle-vertical
	width 8px
	margin-left -8px
	cursor w-resize
